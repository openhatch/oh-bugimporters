//<![CDATA[
var ProjectMap = {
	getStacks: function() {
		$('#a_users').onclick=null;
		$('#a_contributors').onclick=null;

		$('#li_users').addClass('selected');
		$('#li_contributors').removeClass('selected');

		Map.map.clearOverlays();
		$('#map_status').innerHTML='Loading users...';
		Map.defaultIconImage='/images/map_gray.png';
		Map.url='/p/cchost/stacks/near';
		Map.onComplete=function(jsonData){
			if(jsonData && jsonData.accounts && jsonData.accounts.length > 0){
				$('#map_status').html('Showing ' + jsonData.accounts.length +
					' of <a href="https://www.ohloh.net/p/cchost/stacks">0 users</a>.');
			}else{
				$('#map_status').html('No users located.');
			}
			$('#a_contributors').onclick=ProjectMap.getContributors;
		};
		Map.getMarkers();
		return false;
	},
	getContributors: function() {
		$('#a_users').onclick=null;
		$('#a_contributors').onclick=null;

		$('#li_users').removeClass('selected');
		$('#li_contributors').addClass('selected');

		Map.map.clearOverlays();
		$('#map_status').innerHTML='Loading contributors...';
		Map.defaultIconImage='/images/map_yellow.png';
		Map.url='/p/cchost/contributors/near';
		Map.onComplete=function(jsonData){
			if(jsonData && jsonData.accounts && jsonData.accounts.length > 0){
				$('#map_status').html('Showing ' + jsonData.accounts.length +
										' of <a href="https://www.ohloh.net/p/cchost/contributors">8 contributors</a>.');
								}else{
				$('#map_status').html('No contributors located.');
			}
			$('#a_users').bind('click', ProjectMap.getStacks);
		};
		Map.getMarkers();
		return false;
	}
}
//]]>
</script>
